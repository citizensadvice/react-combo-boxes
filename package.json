{
  "name": "@citizensadvice/react-combo-boxes",
  "version": "1.3.1",
  "description": "A combo box implementations in React",
  "license": "ISC",
  "author": "Daniel Lewis",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "exports": {
    ".": "es/index.js",
    "./spec-helpers": "es/spec_helpers/index.js",
    "./helpers": "es/helpers/index.js"
  },
  "files": [
    "es/**/*",
    "cjs/**/*",
    "styles/**/*"
  ],
  "scripts": {
    "build:cjs": "rm -rf cjs && babel src --out-dir cjs --source-maps",
    "build:es": "rm -rf es && BABEL_ENV=es babel src --out-dir es --source-maps",
    "prebuild": "rm -rf dist",
    "build": "parcel build ./examples/index.pug",
    "start": "parcel ./examples/index.pug",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "test": "jest",
    "predeploy": "npm run lint",
    "deploy": "npm run build -- --public-url \"/react-combo-boxes/\" && gh-pages -d dist",
    "preversion": "npm run lint",
    "prepublishOnly": "npm run build:cjs && npm run build:es",
    "postpublish": "npm run deploy",
    "release": "np"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.2.1",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.2.4",
    "marked": "^4.0.10",
    "node-emoji": "^1.11.0",
    "normalize.css": "^8.0.1",
    "np": "^7.5.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-modules": "^4.2.2",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "pug": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.21.0"
  },
  "peerDependencies": {
    "prop-types": ">= 15",
    "react": ">= 16.8"
  },
  "dependencies": {
    "shallow-equal": "^1.2.1"
  },
  "optionalDependencies": {
    "@testing-library/user-event": ">= 4.0.0"
  },
  "sideEffects": false,
  "np": {
    "yarn": false
  }
}
