{
  "name": "@citizensadvice/react-combo-boxes",
  "version": "1.0.0-rc.16",
  "description": "A combo box implementations in React",
  "license": "ISC",
  "author": "Daniel Lewis",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "exports": {
    ".": "es/index.js",
    "./spec-helpers": "es/spec_helpers",
    "./helpers": "es/helpers"
  },
  "files": [
    "es/**/*",
    "cjs/**/*",
    "styles/**/*"
  ],
  "scripts": {
    "build:cjs": "rm -rf cjs && babel src --out-dir cjs --source-maps",
    "build:es": "rm -rf es && BABEL_ENV=es babel src --out-dir es --source-maps",
    "prebuild": "rm -rf dist",
    "build": "parcel build ./examples/index.pug",
    "start": "parcel ./examples/index.pug",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "test": "jest",
    "predeploy": "npm run lint",
    "deploy": "npm run build -- --public-url \"/react-combo-boxes/\" && gh-pages -d dist",
    "preversion": "npm run lint",
    "prepublishOnly": "npm run build:cjs && npm run build:es",
    "postpublish": "npm run deploy",
    "release": "np"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "marked": "^2.0.0",
    "node-emoji": "^1.10.0",
    "normalize.css": "^8.0.1",
    "np": "^7.4.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-modules": "^4.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "pug": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.19.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "prop-types": ">= 15"
  },
  "dependencies": {
    "shallow-equal": "^1.2.1"
  },
  "sideEffects": false,
  "np": {
    "yarn": false
  }
}
