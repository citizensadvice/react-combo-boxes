{
  "name": "@citizensadvice/react-combo-boxes",
  "version": "2.0.0-alpha.3",
  "description": "A combo box implementations in React",
  "license": "ISC",
  "author": "Daniel Lewis",
  "main": "dist/index.js",
  "source": "src/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./spec-helpers": "./dist/spec_helpers/index.js",
    "./helpers": "./dist/helpers/index.js"
  },
  "files": [
    "dist/**/*",
    "styles/**/*"
  ],
  "scripts": {
    "build:js": "rm -rf dist && swc ./src -d dist",
    "prebuild": "rm -rf .gh-pages",
    "build": "parcel build ./examples/index.pug",
    "start": "parcel ./examples/index.pug",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "test": "jest",
    "predeploy": "npm run lint",
    "deploy": "npm run build -- --public-url \"/react-combo-boxes/\" --no-cache && gh-pages -d .gh-pages",
    "preversion": "npm run lint",
    "prepublishOnly": "npm run build:js",
    "postpublish": "npm run deploy",
    "release": "np"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.8.0",
    "@parcel/transformer-pug": "^2.8.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@swc/cli": "^0.1.57",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "gh-pages": "^4.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "marked": "^4.2.3",
    "node-emoji": "^1.11.0",
    "normalize.css": "^8.0.1",
    "np": "^7.6.2",
    "parcel": "^2.8.0",
    "postcss": "^8.4.19",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0"
  },
  "peerDependencies": {
    "prop-types": ">= 15",
    "react": ">= 16.8"
  },
  "dependencies": {
    "shallow-equal": "^1.2.1"
  },
  "optionalDependencies": {
    "@testing-library/user-event": ">= 4.0.0"
  },
  "sideEffects": false,
  "targets": {
    "main": false,
    "default": {
      "distDir": ".gh-pages"
    }
  },
  "np": {
    "yarn": false
  }
}
